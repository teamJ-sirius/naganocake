<div class="container mypage">
  <div class="row">
    <div class="col">
      <h3>配送先編集</h3>
      <%= form_with model: @shipping_address, method: :patch do |f|%>
          <table class="table table-bordered">
          	<tbody>
          		<tr class="table-row">
          			<th class="table-head">郵便番号</th>
          			<th class="table-head">住所</th>
          			<th class="table-head">氏名</th>
          		</tr>
          		<tr>
          			<td class="table-deta"><%= f.text_field :post_code,class: "customer-infomation" %></td>
          			<td class="table-deta"><%= f.text_field :address,class: "customer-infomation" %></td>
          			<td class="table-deta"><%= f.text_field :address_name,class: "customer-infomation" %></td>
          		</tr>
          	</tbody>
          </table>
          <div class="new-shipping_address-botton">
            <%=f.submit "変更内容を保存",class: "btn btn-outline-dark customer-order-detail" %>
          </div>
        <% end %>
      </div>
   </div>
</div>